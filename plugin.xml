<?xml version="1.0" encoding="UTF-8"?>
<plugin id="cordova-admob-sdk" version="0.24.1"
    xmlns="http://cordova.apache.org/ns/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <name>cordova-admob-sdk</name>
    <description>cordova-admob-sdk</description>

    <engines>
        <engine name="cordova" version=">=3.0.0" />
    </engines>

    <platform name="android">
        <framework src="com.google.android.gms:play-services-ads:18.1.1" />
        <framework src="com.facebook.android:audience-network-sdk:5.4.1" />
        <framework src="com.google.ads.mediation:facebook:5.4.1.0" />
               
        <!-- Prevents compilation error: https://developer.android.com/studio/build/multidex.html -->
        <!-- when add additional mediation plugins -->
        <dependency id="cordova-plugin-multidex" />
    </platform>

 </plugin>
